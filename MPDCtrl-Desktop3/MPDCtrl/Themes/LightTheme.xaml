<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="WindowTintColor">#CCCCCC</Color>
    <Color x:Key="WindowFallbackColor">#dddddd</Color>
    <Color x:Key="WindowBackgroundBaseColor">#FFFFFF</Color>
    <Color x:Key="WindowBackgroundColor">#FFFFFF</Color>

    <Color x:Key="ControlForegroundColor">DimGray</Color>
    <Color x:Key="ControlLightColor">DimGray</Color>
    <Color x:Key="ControlMediumColor">Gray</Color>
    <Color x:Key="ControlDarkColor">DarkGray</Color>

    <Color x:Key="ControlMouseOverColor">Black</Color>
    <Color x:Key="ControlMouseOverBackgroundColor">DarkGray</Color>
    <Color x:Key="ControlPressedColor">#7d7d7d</Color>

    <Color x:Key="ControlSelectBackGroundColor">Gainsboro</Color>
    <Color x:Key="ControlSelectBackGroundInactiveColor">DarkGray</Color>

    <Color x:Key="DisabledControlDarkColor">Gray</Color>
    <Color x:Key="DisabledForegroundColor">#FF444444</Color>

    <Color x:Key="BorderLightColor">#212121</Color>
    <Color x:Key="BorderMediumColor">#6c6c6c</Color>
    <Color x:Key="BorderDarkColor">#CCCCCC</Color>

    <Color x:Key="NormalTextColor">#333631</Color>
    <Color x:Key="TitleTextColor">#111111</Color>

    <Color x:Key="NormalTextBoxBackgroundColor">#fefefe</Color>

    <Color x:Key="BoardBackgroundColor">#fcfcfc</Color>

    <Color x:Key="ListviewAlt0Color">#fbfbfb</Color>
    <Color x:Key="ListviewAlt1Color">#f2f2f2</Color>

    <Color x:Key="ListviewItemForegroundColor">#212121</Color>
    <Color x:Key="ListviewItemMouseOverForegroundColor">#333631</Color>
    <Color x:Key="ListviewItemSelectedForegroundColor">Black</Color>


    <Color x:Key="RadioButtonCheckedBackgroundColor">#fcfcfc</Color>
    <Color x:Key="RadioButtonUnCheckedBackgroundColor">#fbfbfb</Color>

    <Color x:Key="ChartVolumeColor">DimGray</Color>

    <Color x:Key="ShadowTabColor">Gainsboro</Color>
    <Color x:Key="ShadowBoardColor">Gainsboro</Color>

    <!-- クール系のアクセントカラー 明るい -->
    <Color x:Key="AccentCoolLightColor">Green</Color>
    <!-- クール系のアクセントカラー　暗め （リストビューのコラム選択背景） -->
    <Color x:Key="AccentCoolDarkColor">#e7e7eb</Color>

    <!-- 暖系のアクセントカラー  明るい -->
    <Color x:Key="AccentWarmLightColor">Orange</Color>
    <!-- 暖系のアクセントカラー 暗め -->
    <Color x:Key="AccentWarmDarkColor">#e0815e</Color>

    <!-- PriceUp Green Blue系 元Aqua -->
    <SolidColorBrush x:Key="PriceUpColorBrush" Color="#00a1e9"/>
    <!-- PriceDown Red Orange系 元Pink -->
    <SolidColorBrush x:Key="PriceDownColorBrush" Color="#e4007f"/>
    <!-- White系 -->
    <SolidColorBrush x:Key="PriceNeutralColorBrush" Color="#6c6c6c"/>

    <SolidColorBrush x:Key="Price24HColorBrush" Color="DarkTurquoise"/>
    <SolidColorBrush x:Key="PriceInitHColorBrush" Color="#a0d8ef"/>
    <SolidColorBrush x:Key="PriceInitLColorBrush" Color="Thistle"/>
    <SolidColorBrush x:Key="Price24LColorBrush" Color="#e597b2"/>

    <!-- 警告　Yellow系 -->
    <SolidColorBrush x:Key="WarningColorBrush" Color="#FFFFBF49"/>

    <!-- スクロールバーの色 #66ffffff -->
    <SolidColorBrush x:Key="ScrollBarColorBrush" Color="#DDDDDD" />
    <!-- ボーダーの色 #66ffffff -->
    <SolidColorBrush x:Key="ScrollBarBorderBrush" Color="Transparent" />
    <!-- トラック（レーン）の色 #33ffffff -->
    <SolidColorBrush x:Key="ScrollBarTrackBrush" Color="Transparent" />
    <!-- 三角の色 -->
    <SolidColorBrush x:Key="ScrollBarHilightBrush" Color="DimGray" />
    <!-- ボタンを押した時の色 #99ffffff -->
    <SolidColorBrush x:Key="ScrollBarPressedBrush" Color="#EEEEEE" />
    <!-- 使用不可の色 -->
    <SolidColorBrush x:Key="ScrollBarDisabledBrush" Color="Gray" />

    <SolidColorBrush x:Key="WindowBorderBrush" Color="#333333" />
    <SolidColorBrush x:Key="WindowBorderInactiveBrush" Color="Gray" />

    <SolidColorBrush x:Key="WindowBackgroundBaseColorBrush" Color="{DynamicResource WindowBackgroundBaseColor}"/>
    <SolidColorBrush x:Key="WindowBackgroundColorBrush" Color="{DynamicResource WindowBackgroundColor}"/>

    <SolidColorBrush x:Key="ControlForegroundColorBrush" Color="{DynamicResource ControlForegroundColor}"/>
    <SolidColorBrush x:Key="ControlLightColorBrush" Color="{DynamicResource ControlLightColor}"/>
    <SolidColorBrush x:Key="ControlMediumColorBrush" Color="{DynamicResource ControlMediumColor}"/>
    <SolidColorBrush x:Key="ControlDarkColorBrush" Color="{DynamicResource ControlDarkColor}"/>
    <SolidColorBrush x:Key="ControlMouseOverColorBrush" Color="{DynamicResource ControlMouseOverColor}"/>
    <SolidColorBrush x:Key="ControlMouseOverBackgroundColorBrush" Color="{DynamicResource ControlMouseOverBackgroundColor}"/>
    <SolidColorBrush x:Key="ControlPressedColorBrush" Color="{DynamicResource ControlPressedColor}"/>

    <SolidColorBrush x:Key="DisabledControlDarkColorBrush" Color="{DynamicResource DisabledControlDarkColor}"/>
    <SolidColorBrush x:Key="DisabledForegroundColorBrush" Color="{DynamicResource DisabledForegroundColor}"/>

    <SolidColorBrush x:Key="NormalTextColorBrush" Color="{DynamicResource NormalTextColor}"/>
    <SolidColorBrush x:Key="TitleTextColorBrush" Color="{DynamicResource TitleTextColor}"/>

    <SolidColorBrush x:Key="NormalTextBoxBackgroundColorBrush" Color="{DynamicResource NormalTextBoxBackgroundColor}"/>

    <SolidColorBrush x:Key="BoardBackgroundColorBrush" Color="{DynamicResource BoardBackgroundColor}"/>

    <SolidColorBrush x:Key="AccentCoolLightColorBrush" Color="{DynamicResource AccentCoolLightColor}"/>
    <SolidColorBrush x:Key="AccentCoolDarkColorBrush" Color="{DynamicResource AccentCoolDarkColor}"/>

    <SolidColorBrush x:Key="AccentWarmDarkColorBrush" Color="{DynamicResource AccentWarmDarkColor}"/>
    <SolidColorBrush x:Key="AccentWarmLightColorBrush" Color="{DynamicResource AccentWarmLightColor}"/>

    <SolidColorBrush x:Key="BorderDarkColorBrush" Color="{DynamicResource BorderDarkColor}"/>

    <SolidColorBrush x:Key="ListviewAlt0ColorBrush" Color="{DynamicResource ListviewAlt0Color}"/>
    <SolidColorBrush x:Key="ListviewAlt1ColorBrush" Color="{DynamicResource ListviewAlt1Color}"/>

    <SolidColorBrush x:Key="ChartVolumeColorBrush" Color="{DynamicResource ChartVolumeColor}" Opacity="0.1"/>

    <SolidColorBrush x:Key="RadioButtonCheckedBackgroundColorBrush" Color="{DynamicResource RadioButtonCheckedBackgroundColor}"/>
    <SolidColorBrush x:Key="RadioButtonUnCheckedBackgroundColorBrush" Color="{DynamicResource RadioButtonUnCheckedBackgroundColor}"/>

    <SolidColorBrush x:Key="ListviewItemForegroundColorBrush" Color="{DynamicResource ListviewItemForegroundColor}"/>
    <SolidColorBrush x:Key="ListviewItemMouseOverForegroundColorBrush" Color="{DynamicResource ListviewItemMouseOverForegroundColor}"/>
    <SolidColorBrush x:Key="ListviewItemSelectedForegroundColorBrush" Color="{DynamicResource ListviewItemSelectedForegroundColor}"/>




    <!-- new none white theme aware -->
    <SolidColorBrush x:Key="ItemBackgroundColorBrush" Color="Transparent"/>
    <Color x:Key="ItemAlt0Color">#212121</Color>
    <SolidColorBrush x:Key="ItemBackgroundAlt0ColorBrush" Color="{DynamicResource ItemAlt0Color}"/>
    <Color x:Key="ItemAlt1Color">#282828</Color>
    <SolidColorBrush x:Key="ItemBackgroundAlt1ColorBrush" Color="{DynamicResource ItemAlt1Color}"/>

    <Color x:Key="ItemSelectedBackgroundColor">DarkGray</Color>
    <SolidColorBrush x:Key="ItemSelectedBackgroundColorBrush" Color="{DynamicResource ItemSelectedBackgroundColor}"/>

    <Color x:Key="ItemSelectedNoneFocusedBackgroundColor">Gray</Color>
    <SolidColorBrush x:Key="ItemSelectedNoneFocusedBackgroundColorBrush" Color="{DynamicResource ItemSelectedNoneFocusedBackgroundColor}"/>
    <SolidColorBrush x:Key="ItemSelectedInactiveBackgroundColorBrush" Color="{DynamicResource ItemSelectedNoneFocusedBackgroundColor}"/>


    <Color x:Key="ItemForegroundColor">#FFCCCCCC</Color>
    <SolidColorBrush x:Key="ItemForegroundColorBrush" Color="{DynamicResource ItemForegroundColor}"/>

    <Color x:Key="ItemSelectedForegroundColor">#191919</Color>
    <SolidColorBrush x:Key="ItemSelectedForegroundColorBrush" Color="{DynamicResource ItemSelectedForegroundColor}"/>

    <Color x:Key="ItemSelectedInactiveForegroundColor">#191919</Color>
    <SolidColorBrush x:Key="ItemSelectedInactiveForegroundColorBrush" Color="{DynamicResource ItemSelectedInactiveForegroundColor}"/>


    <Color x:Key="ItemMouseOverForegroundColor">Gainsboro</Color>
    <SolidColorBrush x:Key="ItemMouseOverForegroundColorBrush" Color="{DynamicResource ItemMouseOverForegroundColor}"/>



</ResourceDictionary>